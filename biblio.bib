@article{CJ99a-u,
	abstract = { The English comparative correlative construction (e.g., The more you eat, the fatter you get) embeds like an ordinary CP, and each of its clauses displays an ordinary long-distance dependency. However, the connection between the two clauses is not ordinary: they are connected paratactically in syntax, but the first clause is interpreted as if it were a subordinate clause. The construction's mixture of the general and the idiosyncratic at all levels of detail challenges the distinction between “core” and “periphery” in grammar and the assumption that some level of underlying syntax directly mirrors semantic structure. },
	author = {Culicover, Peter W. and Jackendoff, Ray},
